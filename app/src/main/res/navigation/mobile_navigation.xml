<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_active">


    <fragment
        android:id="@+id/navigation_active"
        android:name="com.apisubmission.ui.active.ActiveFragment"
        android:label="@string/title_active"
        tools:layout="@layout/fragment_active" />

    <fragment
        android:id="@+id/navigation_finished"
        android:name="com.apisubmission.ui.finished.FinishedFragment"
        android:label="@string/title_finished"
        tools:layout="@layout/fragment_finished" />
</navigation>